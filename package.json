{
  "name": "ioweyou",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "node build.js",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "node -r module-alias/register ./dist --env=production",
    "start:dev": "nodemon",
    "test": "jest",
    "testwatch": "jest --watchAll --testTimeout=10000"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "/react/",
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@shared(.*)$": "<rootDir>/src/shared$1.ts",
      "^@daos(.*)$": "<rootDir>/src/daos$1.ts",
      "^@entities(.*)$": "<rootDir>/src/entities$1.ts"
    }
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@daos": "dist/daos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/Server"
  },
  "dependencies": {
    "@hapi/joi-date": "^2.0.1",
    "bcrypt": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.0.0",
    "http-status-codes": "^1.4.0",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "tedious": "^9.2.1",
    "uuid": "^8.3.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.7",
    "@types/find": "^0.2.1",
    "@types/helmet": "0.0.47",
    "@types/jasmine": "^3.5.12",
    "@types/jest": "^26.0.15",
    "@types/jsonfile": "^6.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@types/multer": "^1.4.4",
    "@types/node": "^14.11.1",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.0",
    "find": "^0.3.0",
    "fs-extra": "^9.0.1",
    "jasmine": "^3.6.1",
    "jest": "^24.9.0",
    "jsonfile": "^6.0.1",
    "nodemon": "^2.0.4",
    "prettier": "2.1.1",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7",
    "widdershins": "^4.0.1"
  },
  "description": "An app for tracking IOUs",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ckesoglou/31242-AIP.git"
  },
  "author": "See AUTHORS.md",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/ckesoglou/31242-AIP/issues"
  },
  "homepage": "https://ioweyou.tech"
}
